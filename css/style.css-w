* {
  box-sizing: border-box;
}

body {
  margin: 0px;
}

h1, h2, h3, h4 {
  margin: 0px;
  padding: 0px;
}

.row {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.dimensione {
  width: 20rem;
  object-fit: cover;
}

.logo {
  width: 80px;
}

.nav {
  display: flex;
  direction: row;
  justify-content: space-around;
  background-color: #da7c36;
  padding-inline: 20px;
}

